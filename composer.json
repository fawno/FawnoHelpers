{
    "name": "fawno/fawno-helpers",
    "description": "Fawno Helpers for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fawno",
            "homepage": "https://github.com/fawno/FawnoHelpers/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/fawno/FawnoHelpers/issues",
        "source": "https://github.com/fawno/FawnoHelpers"
    },
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    },
    "require": {
        "cakephp/cakephp": "^4.5",
        "cakephp/plugin-installer": "^1.0",
        "holt59/cakephp4-bootstrap-helpers": "v3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "FawnoHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FawnoHelpers\\Test\\": "tests/"
        }
    }
}
